# Configuración de Obstáculos para el Planificador de Rutas
# Formato: TIPO: datos
# Líneas que empiecen con # son comentarios

# Configuración del grid (minX, minY, maxX, maxY)
GRID: 0, 0, 1000, 1000

# Obstáculos puntuales (x, y)
PUNTO: 100, 100
PUNTO: 150, 150
PUNTO: 200, 200

# Líneas horizontales (y, xInicio, xFin)
# Ejemplo: línea horizontal en y=300 desde x=100 hasta x=400
LINEA_H: 300, 100, 400
LINEA_H: 450, 200, 600
LINEA_H: 600, 50, 150

# Líneas verticales (x, yInicio, yFin)  
# Ejemplo: línea vertical en x=250 desde y=100 hasta y=500
LINEA_V: 250, 100, 500
LINEA_V: 500, 200, 700
LINEA_V: 750, 300, 800

# Polígonos abiertos (secuencia de puntos separados por ;)
# Ejemplo: polígono en forma de L
POLIGONO: 600,600; 600,650; 650,650; 650,700; 700,700

# Ejemplo: polígono zigzag
POLIGONO: 800,100; 850,150; 900,100; 950,150; 1000,100

# Obstáculos que simulan edificios o zonas restringidas
LINEA_H: 100, 300, 350
LINEA_V: 300, 100, 150
LINEA_V: 350, 100, 150
LINEA_H: 150, 300, 350

# Obstáculo en forma de cruz
LINEA_H: 500, 400, 600
LINEA_V: 500, 450, 550

# Zona de almacenes (libre de obstáculos en las esquinas)
# Esquina inferior izquierda: (0,0) a (50,50) - libre
# Esquina superior derecha: (950,950) a (1000,1000) - libre

# Carretera principal bloqueada en ciertos tramos
LINEA_H: 200, 0, 200
LINEA_H: 400, 800, 1000
LINEA_V: 600, 0, 300